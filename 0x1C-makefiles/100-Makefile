CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
CFLAGS = -Wall -Werror -Wextra -pedantic

all: $(OBJ) m.h
	$(CC) -Wall -Werror -Wextra -pedantic $(OBJ) -o $(NAME)

.PHONY: all clean oclean fclean re

clean:
	-$(RM) *~ $(NAME)
oclean:
	-$(RM) $(OBJ)
fclean: clean oclean

re: oclean all

m.h:
	@false
